{
  "name": "stimulus-slimselect",
  "version": "0.2.0",
  "description": "A Stimulus wrapper around Slimselect",
  "main": "dist/stimulus-slimselect.js",
  "umd:main": "dist/stimulus-slimselect.umd.js",
  "module": "dist/stimulus-slimselect.m.js",
  "esmodule": "dist/stimulus-slimselect.modern.js",
  "repository": {
    "url": "https://github.com/excid3/stimulus-slimselect",
    "type": "git"
  },
  "author": "Chris Oliver <excid3@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "private": false,
  "amdName": "StimulusSlimSelect",
  "keywords": [
    "stimulus",
    "stimulusjs",
    "controller",
    "slimselect",
    "components"
  ],
  "np": {
    "tests": false
  },
  "peerDependencies": {
    "@hotwired/stimulus": ">=3.0.0",
    "slim-select": "^1.27.0"
  },
  "scripts": {
    "build": "microbundle --globals stimulus=Stimulus",
    "prepublish": "yarn build",
    "test": "NODE_ENV=test jest"
  },
  "jest": {
    "testRegex": ".*_test.js",
    "roots": [
      "__tests__"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.10",
    "babel-jest": "^26.6.3",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^26.6.3",
    "microbundle": "^0.12.3",
    "mutationobserver-shim": "^0.3.7"
  }
}
